<?xml version="1.0" encoding="utf-8"?>
<com.avc.manager.Res.HoldebleObject
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/status_rv_viewLinearLayout"
	android:layout_width="fill_parent"
	android:layout_height="200dp"
	android:orientation="vertical"
	android:layout_margin="5dp"
	android:background="@drawable/light_border_background">

	<RelativeLayout
		android:id="@+id/status_rv_viewRelativeLayout"
		android:layout_margin="5dp"
		android:layout_width="match_parent"
		android:layout_height="match_parent">

		<ImageView
			android:id="@+id/preview_layoutImageView"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:gravity="center"
			android:scaleType="centerCrop"/>

		<VideoView
			android:id="@+id/preview_layoutVideoView"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:gravity="center"
			android:layout_gravity="center"
			android:layout_centerInParent="true"
			android:scaleType="centerCrop"/>

	</RelativeLayout>

</com.avc.manager.Res.HoldebleObject>

